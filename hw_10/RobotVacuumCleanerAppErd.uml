User {
	idUser integer pk increments
	hashIoginPasword text
	name text
	e-mail text
}

Robots {
	idRobot integer pk increments
	model text
	fabricNum text
	idGroup integer *> Grour.idGrour
	version text
	ipAddress integer
	statistica integer
}

Grour {
	idGrour integer pk increments
	group text
	idUser1 text *> User.idUser
	idUser2 text *> User.idUser
	idUser3 text *> User.idUser
}

Schedule {
	idSchedule integer pk increments
	IdRobots integer >* Robots.idRobot
	dateTime date
	idTypeJ integer >* TypeJ.idTypeJ
}

TypeJ {
	idTypeJ integer pk increments > Schedule.id
	nameType integer
}
